#include <stdio.h>
int main() 
{
  int i, n;
  scanf("%d",&n);
  int arr[n];
  for(i=0;i<n;i++)
  {
      printf("the given mark that stored in index %d :",i);
      scanf("%d",&arr[i]);
      printf("\n");
  }
  int tot=0;
  for(i=0;i<n;i++)
  {
      tot+=arr[i];
  }
  printf("the total is :%d",tot);
}
