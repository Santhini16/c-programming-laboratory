#include<string.h>
#include <stdio.h>
int main() 
{
   char a[20],b[20];
   scanf("%s%s",a,b);
   int l1=0,l2=0;
   while(a[l1]!='\0')
   {
       l1++;
   }
   while(a[l2]!='\0')
   {
       l2++;
   }
   char c[l1+l2];
   int i=0,j=0,k=0;
   while(a[i]!='\0')
   {
       c[k]=a[i];
       i++;
       k++;
   }
   while(b[j]!='\0')
   {
       c[k]=b[j];
       j++;
       k++;
   }
   c[k]='\0';
 printf("%s\n",c);
 printf("%d",k);
}
