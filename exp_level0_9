#include <stdio.h>
int main() 
{
    int a,b,c;
    scanf("%d%d%d",&a,&b,&c);
    if(a<b && a<c)
    {
                printf("the smallest is %d",a);
            }
    else if(b<a && b<c)
    {
                printf("the smallestis %d",b);
    }
    else
    {
        printf("the smallest is %d",c);
    }
}
